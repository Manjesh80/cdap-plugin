{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "DMaap Stream Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Host Name",
          "name": "dmaapHostName",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Topic Name",
          "name": "dmaapTopicName",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Protocol",
          "name": "dmaapProtocol",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "dmaapUserName",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Password",
          "name": "dmaapUserPassword",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Content Type",
          "name": "dmaapContentType",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Consumer ID",
          "name": "dmaapConsumerId",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Consumer Group",
          "name": "dmaapConsumerGroup",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Timeout in MS",
          "name": "dmaapTimeoutMS",
          "widget-attributes" : {
            "default": "5"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Message Limit",
          "name": "dmaapMessageLimit",
          "widget-attributes" : {
            "default": "10"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "textbox",
          "label": "URL",
          "name": "url",
          "widget-attributes" : {
            "default": "XXXXXX"
          }
        },
        {
          "widget-type": "number",
          "label": "Interval",
          "name": "interval",
          "widget-attributes" : {
            "default": "1",
            "min": "1"
          }
        },
        {
          "widget-type": "keyvalue",
          "label": "Request Headers",
          "name": "requestHeaders",
          "widget-attributes" : {
            "showDelimiter": "false",
            "kv-delimiter" : ":",
            "delimiter" : "\n"
          }
        },
        {
          "widget-type": "select",
          "label": "Charset",
          "name": "charset",
          "widget-attributes" : {
            "values": [
              "ISO-8859-1",
              "US-ASCII",
              "UTF-8",
              "UTF-16",
              "UTF-16BE",
              "UTF-16LE"
            ],
            "default": "UTF-8"
          }
        },
        {
          "widget-type": "select",
          "label": "Should Follow Redirects",
          "name": "followRedirects",
          "widget-attributes" : {
            "values": [
              "true",
              "false"
            ],
            "default": "true"
          }
        },
        {
          "widget-type": "number",
          "label": "Connect Timeout",
          "name": "connectTimeout",
          "widget-attributes" : {
            "default": "60000",
            "min": "0"
          }
        },
        {
          "widget-type": "number",
          "label": "Read Timeout",
          "name": "readTimeout",
          "widget-attributes" : {
            "default": "60000",
            "min": "0"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "MESSAGE_NUM" : "long",
        "MESSAGE" : "string"
      }
    }
  ]
}
